{% block head_scripts %}
    <script src="https://maps.googleapis.com/maps/api/js?v=3.exp&amp;sensor=false&amp;libraries=places"></script>
    <script src="{{ asset('js/dist/head.js') }}"></script>
    {% if app.debug == true %}
        <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>
        <script src="{{ path('thumbor_configuration') }}"></script>
    {% endif %}

    <script type="text/javascript">
        var VOLO = VOLO || {};
        VOLO.Translations = {
            home: {
                postal_index_form_please_select_option: '{{ 'home.postal_index_form_please_select_option'|trans|e }}'
            }
        };
        VOLO.configuration = VOLO.configuration || {};
        VOLO.configuration.countryCode = '{{ country_code }}';
        VOLO.configuration.locale = '{{ app.request.locale|replace({'_': '-'}) }}';

        {% set config = get_configuration() %}
        {% set userLocale = app.request.locale|replace({'_': '-'}) %}
        $(document).ready(function () {
            VOLO.initIntl('{{ userLocale }}', VOLO.configuration.locale, '{{ config.currency.currency_symbol_iso }}');
        });
    </script>

{% endblock %}
