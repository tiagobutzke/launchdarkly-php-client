<div class="modal" id="allergyModal" tabindex="-1" role="dialog" aria-labelledby="allergyModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="allergyModalLabel">{% trans %}Allergy Table{% endtrans %}</h4>
            </div>
            <div class="modal-body">
                <ul>
                    <li>4 {% trans %}allergy.eggs{% endtrans %}</li>
                    <li>5 {% trans %}allergy.milk{% endtrans %}</li>
                    <li>6 {% trans %}allergy.celery{% endtrans %}</li>
                    <li>7 {% trans %}allergy.sesame{% endtrans %}</li>
                    <li>8 {% trans %}allergy.fish{% endtrans %}</li>
                    <li>9 {% trans %}allergy.peanuts{% endtrans %}</li>
                    <li>10 {% trans %}allergy.nuts{% endtrans %}</li>
                    <li>11 {% trans %}allergy.mustard{% endtrans %}</li>
                    <li>12 {% trans %}allergy.hot1{% endtrans %}</li>
                    <li>13 {% trans %}allergy.hot2{% endtrans %}</li>
                    <li>14 {% trans %}allergy.hot3{% endtrans %}</li>
                    <li>15 {% trans %}allergy.vegetarian{% endtrans %}</li>
                    <li>16 {% trans %}allergy.crustaceans{% endtrans %}</li>
                    <li>17 {% trans %}allergy.cereals_containing_gluten{% endtrans %}</li>
                    <li>18 {% trans %}allergy.lupin{% endtrans %}</li>
                    <li>19 {% trans %}allergy.sulphur_dioxide_and_sulphites{% endtrans %}</li>
                    <li>20 {% trans %}allergy.soy{% endtrans %}</li>
                    <li>21 {% trans %}allergy.molluscs{% endtrans %}</li>
                    <li>22 {% trans %}allergy.vegan{% endtrans %}</li>
                </ul>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
