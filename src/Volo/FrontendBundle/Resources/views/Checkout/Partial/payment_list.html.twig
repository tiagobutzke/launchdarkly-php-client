{% for card in customer_cards %}
    <div class="checkout__list">
        <label>
            <input type="radio" name="credit_card" value="{{ card.id }}"/>
            <div class="checkout__list__items">{{ card.number }} {{ card.holder_name }}, {{ card.expiry_month }}/{{ card.expiry_year }}</div>
        </label>
    </div>
{% endfor %}
