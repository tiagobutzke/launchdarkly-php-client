<h3 class="checkout__title">
    2. {% trans %}Contact info{% endtrans %}
    <span class="checkout__subtitle__edit">
        <i class="icon-pencil"></i>
        <a href="{{ path('checkout_contact_information', {vendorCode: vendor.code}) }}">{% trans %}Edit{% endtrans %}</a>
    </span>
</h3>
<div class="checkout__item checkout__item--no-subtitle">
    <span>{{ customer.first_name }} {{ customer.last_name }}</span>
    <span>{{ customer.email }}</span>
    <span>{{ customer.mobile_country_code }}{{ customer.mobile_number }}</span>
</div>
