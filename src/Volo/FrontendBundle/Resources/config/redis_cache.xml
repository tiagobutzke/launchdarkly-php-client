<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="volo_frontend_redis_connection_service" class="%doctrine_cache.redis.connection.class%">
            <call method="connect">
                <argument>%redis_cache_host%</argument>
                <argument>%redis_cache_port%</argument>
            </call>
        </service>
    </services>

</container>
